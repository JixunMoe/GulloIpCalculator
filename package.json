{
  "name": "gulloipcalculator",
  "version": "1.0.0",
  "description": "",
  "main": "rollup.config.js",
  "scripts": {
    "build:dev": "cross-env ROLLUP_WATCH=1 NODE_ENV=development rollup -c",
    "build:prod": "cross-env NODE_ENV=production rollup -c",
    "clean": "rm public/*.js public/*.map",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "serve public"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.15.6",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-filesize": "^6.1.1",
    "rollup-plugin-node-resolve": "^5.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.0.0",
    "serve": "^11.0.1"
  },
  "keywords": [
    "Gullo",
    "Calculator"
  ],
  "author": "Jixun",
  "license": "MIT"
}
